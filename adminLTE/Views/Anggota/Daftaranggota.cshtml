@model adminLTE.ViewModel.AnggotaOrmawaViewModel

@{
    ViewData["Title"] = "Daftar Anggota";
    Layout = "~/Views/Shared/_MainMenu.cshtml";
}
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Daftar Anggota
        <small>advanced tables</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="https://localhost:44384/Home/Beranda"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Daftar Anggota</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-body">
                    <div class="dataTables_wrapper form-inline dt-bootstrap" id="example1_wrapper">
                        <div class="row">
                            <div class="col-sm-6">
                                <div id="example1_length" class="dataTables_length">
                                    <label>
                                        Show
                                        <select class="form-contro l input-sm" aria-controls="example1" name="example1_length">
                                            <option value="10">10</option>
                                            <option value="25">25</option>
                                            <option value="50">50</option>
                                            <option value="100">100</option>
                                        </select>
                                        entries
                                    </label>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div id="example1_filter" class="dataTables_filter">
                                    <label>
                                        Search:
                                        <input aria-controls="example1" placeholder="" class="form-control input-sm" type="search">
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <table aria-describedby="example1_info" role="grid" id="example1" class="table table-bordered table-striped dataTable">
                                    <thead>
                                        <tr>
                                            <th>
                                                @Html.DisplayNameFor(model => model.MahasiswaId)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.OrganisasiOrmawaId)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.TanggalBergabung)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.StatusAnggota)
                                            </th>
                                            <th>
                                                <a asp-action="AddOrEdit" class="btn btn-outline-success"><i class="far fa-plus-square"></i>Add Anggota</a>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @*@foreach (var item in Model)
                                        {*@
                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(modelItem => modelItem.MahasiswaId)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => modelItem.OrganisasiOrmawaId)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => modelItem.TanggalBergabung)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => modelItem.StatusAnggota)
                                                </td>
                                                @*<td>
                                                    <a asp-action="AddOrEdit" asp-route-id="@item.AnggotaID">Edit</a> |
                                                    <a asp-action="Delete" asp-route-id="@item.AnggotaID" onclick="return confirm('Are you sure to delete this record?')">Delete</a>
                                                </td>*@
                                            </tr>
                                        @*}*@
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
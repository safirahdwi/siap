                                            <td>
                                                @Html.DisplayFor(modelItem => modelItem.DanaAnggaran)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => modelItem.PenanggungJawabId)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => modelItem.ApprovedBy)

                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.PenanggungJawabId)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.ApprovedBy)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.TimeApproved)
                                            </th>
                                            <th>
                                                <a asp-action="AddOrEdit" class="btn btn-outline-success"><i class="far fa-plus-square"></i>Add Anggota</a>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @*@foreach (var item in Model)
